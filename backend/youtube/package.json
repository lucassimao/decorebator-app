{
  "name": "decorebator-youtube",
  "version": "0.0.1",
  "description": "Set of utilities to interact with youtube",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test SHOW_LOG_ON_STDOUT=true MONGO_DB_URL=mongo://localhost:27017/decorebator-tests mocha 'src/**/__tests__/*' -R spec",
    "lint": "eslint 'src/**/*.js'",
    "jest-watch": "jest --watch --detectOpenHandles 'src/'",
    "start": "nodemon --trace-warnings src/index.js",
    "format": "prettier --write 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lucassimao/decorebator-app.git"
  },
