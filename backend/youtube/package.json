{
  "name": "decorebator-youtube",
  "version": "0.0.1",
  "description": "Set of utilities to interact with youtube",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "jest-watch": "jest --watch --detectOpenHandles 'src/'",
    "format": "prettier --write 'src/**/*.js'",
    "start:dev": "nodemon",
    "build": "npx rimraf build && npx tsc",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lucassimao/decorebator-app.git"
  },
  "author": "Lucas Sim√£o",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucassimao/decorebator-app/issues"
  },
  "homepage": "https://github.com/lucassimao/decorebator-app#readme",
  "dependencies": {
    "apollo-server": "^2.25.0",
    "class-validator": "^0.12.2",
    "gcp-metadata": "^4.2.1",
    "graphql": "^15.3.0",
    "jsonwebtoken": "^8.5.1",
    "metadata-scraper": "^0.2.23",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --cache --fix"
    ],
    "src/**/*.{js,ts,css,md}": [
      "prettier --write"
    ]
  }
}
