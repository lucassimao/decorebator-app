  "bugs": {
    "url": "https://github.com/lucassimao/decorebator-app/issues"
  },
  "homepage": "https://github.com/lucassimao/decorebator-app#readme",
  "dependencies": {
    "apollo-server": "^2.14.1",
    "graphql": "^15.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "nodemon": "^2.0.4",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
